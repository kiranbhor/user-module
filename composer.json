{
  "name": "accunity/user-module",
  "type": "asgard-module",
  "description": "User module for AsgardCMS. Handles the authentication and authorisation as well as the user management.(forked)",
  "keywords": [
    "asgardcms",
    "user",
    "authentication",
    "authorisation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kiran Bhor",
      "email": "kiranbhor05@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/User"
  },
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "ramsey/uuid": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*",
    "scrutinizer/ocular": "~1.1",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Modules\\User\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\User\\Tests\\": "Tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
